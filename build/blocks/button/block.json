{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "xapp/button",
  "version": "0.1.0",
  "title": "Button",
  "category": "brand",
  "icon": "button",
  "description": "Add a button block",
  "supports": {
    "renaming": true,
    "className": true,
    "anchor": false,
    "reusable": false,
    "html": false,
    "color": {
      "gradients": true,
      "link": true,
      "__experimentalDefaultControls": {
        "background": true,
        "text": false
      }
    },
    "spacing": {
      "blockGap": true,
      "padding": true,
      "margin": true,
      "__experimentalDefaultControls": {
        "padding": true
      }
    },
    "dimensions": {
      "aspectRatio": true,
      "minHeight": true
    },
    "__experimentalBorder": {
      "radius": true,
      "color": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "radius": true,
        "color": true,
        "style": true,
        "width": true
      }
    },
    "__experimentalLayout": false
  },
  "textdomain": "mdc-tabs",
  "editorScript": "file:./index.js",
  "styles": [
    {
      "name": "fill",
      "label": "Fill",
      "isDefault": true
    },
    {
      "name": "outline",
      "label": "Outline"
    },
    {
      "name": "text",
      "label": "Text"
    },
    {
      "name": "icon",
      "label": "Icon"
    }
  ],
  "attributes": {
    "action": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "linkTo": {
      "type": "string",
      "default": ""
    },
    "screenType": {
      "type": "string",
      "default": ""
    },
    "templatekey": {
      "type": "string",
      "default": ""
    },
    "queryId": {
      "type": "string",
      "default": ""
    },
    "height": {
      "type": "number",
      "default": 50
    },
    "width": {
      "type": "number",
      "default": 150
    },
    "size": {
      "type": "string",
      "default": "custom"
    },
    "selfStretch": {
      "type": "string",
      "default": ""
    },
    "style": {
      "type": "object",
      "default": {
        "spacing": {
          "width": "150px",
          "height": "50px"
        },
        "elevation": 0,
        "color": {
          "background": "var(--wp--theme--color--primary)"
        }
      }
    },
    "align": {
      "type": "string",
      "default": "center"
    }
  },
  "editorStyle": "wp-xapp-button-editor",
  "style": "wp-xapp-button"
}