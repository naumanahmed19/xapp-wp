{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "xapp/bottom-tabs",
  "version": "0.1.0",
  "title": "Bottom Tabs",
  "category": "design",
  "icon": "layout",
  "description": "Brand App tabs and general settings.",
  "supports": {
    "html": false,
    "multiple": false,
    "alignWide": false,
    "color": {
      "gradients": false,
      "link": false,
      "__experimentalDefaultControls": {
        "background": true,
        "text": false
      }
    }
  },
  "textdomain": "mdc-tabs",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "styles": [
    {
      "name": "default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "other",
      "label": "Other"
    }
  ],
  "attributes": {
    "settings": {
      "type": "object",
      "default": {}
    },
    "showSelectedLabels": {
      "type": "boolean",
      "default": false
    },
    "showUnselectedLabels": {
      "type": "boolean",
      "default": false
    },
    "tabs": {
      "type": "array",
      "default": [
        {
          "tabId": "home",
          "index": 0,
          "title": "Home Page",
          "title_key": "",
          "icon": {
            "code": "e88a",
            "name": "home"
          }
        },
        {
          "tabId": "search",
          "index": 1,
          "title": "Search",
          "title_key": "",
          "icon": {
            "code": "e8b6",
            "name": "search"
          }
        },
        {
          "tabId": "favourites",
          "index": 2,
          "title": "Favourites",
          "title_key": "",
          "icon": {
            "code": "e87d",
            "name": "favorite"
          }
        },
        {
          "tabId": "account",
          "index": 3,
          "title": "Account",
          "title_key": "",
          "icon": {
            "code": "e853",
            "name": "account_circle"
          }
        }
      ]
    },
    "style": {
      "type": "object",
      "default": {}
    },
    "align": {
      "type": "string",
      "default": "center"
    }
  }
}