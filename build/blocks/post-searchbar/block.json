{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "xapp/post-searchbar",
  "version": "0.1.0",
  "title": "Post Search bar",
  "category": "brand",
  "icon": "layout",
  "description": "A search bar",
  "supports": {
    "html": false,
    "alignWide": false,
    "align": [
      "center",
      "full",
      "left",
      "right",
      "wide"
    ],
    "color": {
      "gradients": false,
      "link": true,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    },
    "__experimentalBorder": {
      "radius": true,
      "color": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "radius": true,
        "color": true,
        "style": true,
        "width": true
      }
    }
  },
  "textdomain": "xapp-brand",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "styles": [
    {
      "name": "default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "other",
      "label": "Other"
    }
  ],
  "attributes": {
    "queryId": {
      "type": "string",
      "default": ""
    },
    "autofocus": {
      "type": "boolean",
      "default": false
    },
    "placeholder": {
      "type": "string",
      "default": ""
    },
    "style": {
      "type": "object",
      "default": {}
    },
    "align": {
      "type": "string",
      "default": "center"
    }
  }
}