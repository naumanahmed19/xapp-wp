{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "xapp/appbar",
  "version": "0.1.0",
  "title": "Appbar",
  "category": "layout",
  "icon": "layout",
  "description": "An app bar for each main screen",
  "supports": {
    "html": false,
    "alignWide": false,
    "reusable": true,
    "color": {
      "gradients": true,
      "link": false,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    }
  },
  "textdomain": "mdc-tabs",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "styles": [
    {
      "name": "default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "other",
      "label": "Other"
    }
  ],
  "attributes": {
    "pinned": {
      "type": "boolean",
      "default": true
    },
    "height": {
      "type": "string",
      "default": ""
    },
    "backgroundImage": {
      "type": "object",
      "default": {}
    },
    "style": {
      "type": "object",
      "default": {
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}