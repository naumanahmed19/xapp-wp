{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "xapp/logo",
  "version": "0.1.0",
  "title": "Logo",
  "category": "brand",
  "icon": "layout",
  "description": "Add a logo block",
  "supports": {
    "html": false,
    "alignWide": false,
    "align": []
  },
  "textdomain": "mdc-tabs",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "logo": {
      "type": "object",
      "default": {}
    },
    "logoLight": {
      "type": "object",
      "default": {}
    },
    "height": {
      "type": "number",
      "default": 50
    },
    "width": {
      "type": "number",
      "default": 50
    },
    "style": {
      "type": "object",
      "default": {}
    },
    "align": {
      "type": "string",
      "default": ""
    }
  }
}