{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "xapp/screens",
  "version": "0.1.0",
  "title": "App Screens",
  "category": "design",
  "icon": "layout",
  "description": "Add app screens.",
  "supports": {
    "html": false,
    "multiple": false,
    "anchor": true
  },
  "textdomain": "mdc-tabs",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "styles": [],
  "attributes": {
    "blockId": {
      "type": "string"
    },
    "activeScreenIndex": {
      "type": "number",
      "default": 0
    },
    "editMode": {
      "type": "boolean",
      "default": false
    },
    "guide": {
      "type": "boolean",
      "default": false
    },
    "settings": {
      "type": "object",
      "default": {
        "action": "new",
        "screens": []
      }
    },
    "style": {
      "type": "object",
      "default": {}
    }
  }
}