{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "xapp/inkwell",
  "version": "0.1.0",
  "title": "Clickable",
  "category": "brand",
  "keywords": [
    "inkwell,button,click,tap,touch"
  ],
  "icon": "link",
  "description": "Make a block or section clickable",
  "supports": {
    "anchor": true,
    "align": true,
    "alignWide": false
  },
  "textdomain": "xapp",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "settings": {
      "type": "object",
      "default": {
        "title": "",
        "label": "",
        "action": "",
        "linkTo": "",
        "screenType": "",
        "auth": false
      }
    }
  }
}