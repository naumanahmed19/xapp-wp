{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "xapp/positioned",
  "version": "0.1.0",
  "title": "Positioned",
  "category": "brand",
  "icon": "aspectRatio",
  "description": "A block for displaying a centered content",
  "supports": {
    "html": false,
    "alignWide": false,
    "reusable": true
  },
  "textdomain": "mdc-tabs",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "styles": [
    {
      "name": "default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "other",
      "label": "Other"
    }
  ],
  "attributes": {
    "position": {
      "type": "object",
      "default": {
        "top": "0px",
        "left": "0px",
        "right": "0px",
        "bottom": "0px"
      }
    },
    "style": {
      "type": "object",
      "default": {
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}