            "title_key": "",
            "icon": {
              "code": 59664,
              "name": "circle",
              "uid": "95daccc64b347c483446a8b070470756"
            }
          },
          {
            "tabId": "slide2",
            "index": 1,
            "title": "Slide 2",
            "title_key": "",
            "icon": {
              "code": 59664,
              "name": "circle",
              "uid": "95daccc64b347c483446a8b070470756"
            }
          }
        ]
      }
    },
    "style": {
      "type": "object",
      "default": {}
    },
    "align": {
      "type": "string",
      "default": "center"
    }
  }
}