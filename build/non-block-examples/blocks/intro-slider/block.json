{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "xapp/intro-slider",
  "version": "0.1.0",
  "title": "Intro Slider",
  "category": "design",
  "icon": "layout",
  "description": "Brand App tabs and general settings.",
  "supports": {
    "html": false,
    "multiple": false,
    "alignWide": false,
    "align": [
      "center",
      "full",
      "left",
      "right",
      "wide"
    ]
  },
  "textdomain": "mdc-tabs",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "styles": [
    {
      "name": "default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "other",
      "label": "Other"
    }
  ],
  "attributes": {
    "blockId": {
      "type": "string"
    },
    "settings": {
      "type": "object",
      "default": {
        "showSelectedLabels": false,
