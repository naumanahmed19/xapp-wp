{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "xapp/icon-button",
  "version": "0.1.0",
  "title": "Icon Button",
  "category": "brand",
  "keywords": [
    "inkwell,button,click,tap,touch"
  ],
  "icon": "link",
  "description": "Make a block or section clickable",
  "supports": {
    "align": true
  },
  "textdomain": "xapp",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "action": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "linkTo": {
      "type": "string",
      "default": ""
    },
    "screenType": {
      "type": "string",
      "default": ""
    },
    "templatekey": {
      "type": "string",
      "default": ""
    },
    "queryId": {
      "type": "string",
      "default": ""
    }
  }
}