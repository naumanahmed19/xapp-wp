{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "xapp/screen",
  "version": "0.1.0",
  "title": "Screen",
  "category": "design",
  "icon": "mobile",
  "description": "Example static block scaffolded with Create Block tool.",
  "supports": {
    "html": false,
    "lock": true,
    "anchor": false
  },
  "parent": [
    "xapp/screens"
  ],
  "textdomain": "mdc-tabs",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "verticalAlignment": {
      "type": "string"
    },
    "allowedBlocks": {
      "type": "array"
    },
    "templateLock": {
      "type": [
        "string",
        "boolean"
      ],
      "enum": [
        "all",
        false
      ]
    },
    "style": {
      "type": "object",
      "default": {}
    },
    "tabScreenIndex": {
      "type": "number"
    },
    "icon": {
      "type": "object",
      "default": {}
    },
    "title": {
      "type": "string"
    },
    "scroll": {
      "type": "boolean",
      "default": true
    }
  }
}