{
	"name": "xapp-wp",
	"version": "0.1.0",
	"description": "Example static block scaffolded with Create Block tool.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"start": "npm-run-all --parallel start:block start:non-block tailwindwatch",
		"sync": "browser-sync start -p 'myexample.local' --files '**/*.php' 'build/*.js' 'build/*.css'",
		"start:block": "wp-scripts start --webpack-copy-php --webpack-src-dir=src/blocks --output-path=build/blocks",
		"start:non-block": "wp-scripts start --output-path=build/non-block-examples --config=./non-block-examples.webpack.config.js",
		"build": "wp-scripts build --webpack-copy-php --webpack-src-dir=src/blocks --output-path=build/blocks",
		"build:non-block": "wp-scripts build --output-path=build/non-block-examples --config=./non-block-examples.webpack.config.js",
		"build:all": "npm-run-all --parallel build && build:non-block && tailwindbuild",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"lint:js": "wp-scripts lint-js",
		"lint:style": "wp-scripts lint-style",
		"format:js": "wp-scripts format",
		"lint:js:fix": "npm run lint:js -- --fix",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"test": "wp-scripts test-unit-js",
		"env:start": "wp-env start",
		"env:start:debug": "wp-env start --xdebug",
		"env:stop": "wp-env stop",
		"packages-update": "wp-scripts packages-update",
		"tailwindbuild": "tailwindcss -i ./src/index.css -o ./build/index.css --postcss --minify",
		"tailwindwatch": "tailwindcss -i ./src/index.css -o ./build/index.css --watch --postcss --minify"
	},
	"devDependencies": {
		"@babel/core": "^7.18.9",
		"@babel/eslint-parser": "^7.18.9",
		"@headlessui/react": "^1.6.5",
		"@heroicons/react": "^1.0.6",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"@wordpress/env": "^4.6.0",
		"@wordpress/scripts": "^23.2.0",
		"autoprefixer": "^10.4.7",
		"browser-sync": "^2.27.10",
		"copy-webpack-plugin": "^10.2.4",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "28.1.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.14",
		"postcss-nested": "^5.0.6",
		"prettier": "^2.3.2",
		"prisma": "^4.1.0",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"tailwindcss": "^3.1.7",
		"terser-webpack-plugin": "^5.3.6",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "4.0.0",
		"typescript": "^4.3.5",
		"wp-prettier": "^2.6.2"
	},
	"dependencies": {
		"@types/jest": "^28.1.6",
		"@types/node": "^18.6.4",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"@wordpress/api-fetch": "^6.8.0",
		"@wordpress/icons": "^9.4.0",
		"@wordpress/notices": "^3.14.0",
		"axios": "^0.27.2",
		"dayjs": "^1.11.4",
		"locale-codes": "^1.3.1"
	}
}
